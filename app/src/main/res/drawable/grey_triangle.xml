<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <rotate
            android:fromDegrees="-6"
            android:toDegrees="45"
            android:pivotX="100%"
            android:pivotY="0%" >
            <shape
                android:shape="rectangle" >
                <stroke android:color="@color/colorGreyBackground" android:width="10dp"/>
                <solid
                    android:color="@color/colorGreyBackground" />
            </shape>
        </rotate>
    </item>
</layer-list>